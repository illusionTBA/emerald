  "scripts": {
    "dev": "concurrently -k -n \"Bare,Next\" -p \"[{name}]\" -c \"blue,magenta\" \"bare-server-node --port 3001 --host 0.0.0.0 -e\" \"next dev\"",
    "build": "next build",
    "start": "concurrently -k -n \"Bare,Next\" -p \"[{name}]\" -c \"blue,magenta\" \"bare-server-node --port 3001 --host 0.0.0.0 -e\" \"next start -p 3005\"",
    "lint": "next lint",
    "bsw": "workbox-cli generateSW workbox-config.js",
    "bstartold": "next build && concurrently -k -n \"Bare,Next\" -p \"[{name}]\" -c \"blue,magenta\" \"bare-server-node --port 3001 --host 0.0.0.0 -e\" \"next start -p 3005\"",
